<view class="bigwrap">
  <view class="head">
    <view class="headimgwrap">
      <image src="/img/lyf.jpg" class="headimg"></image>
      <view class="maskwrap">
        <view class="mask">
            <image src="/img/camera.png"></image>
        </view>
        
      </view>
    </view>
    <view class="usermsg">
      <text class="username bigfont">莉莉安</text>
      <text class="percentage smallerfont">您的资料完整的100%</text>
    </view>
    <button class="smallfont">切换到媒婆</button>
  </view>
  <!--相册198  -->
  <view class="album">
    <text>相册</text>
    <view class="classname">
      <image src="/img/lyf.jpg"></image>
      <image src="/img/lyf.jpg"></image>
      <image src="/img/lyf.jpg"></image>
      <image src="/img/addimg.png" class="addimg"></image>
    </view>
  </view>

  <!--媒豆基金  -->
  <view class="beansmoney">  
      <view class="allwrap" bindtap="gobeans"><text class="tilte">我的媒豆</text><text class="arrow"></text></view>
      <view class="allwrap" bindtap="gomoney"><text class="tilte">幸福基金</text><text class="arrow"></text></view>
  </view>
  <!--其它  -->

  <view class="">  
      <view class="allwrap" bindtap=""><text class="tilte">我的媒婆</text><text class="arrow"></text></view>
      <view class="allwrap" bindtap=""><text class="tilte">关注的媒婆</text><text class="arrow"></text></view>
      <view class="allwrap" bindtap=""><text class="tilte">编辑资料</text><text class="arrow"></text></view>
      
      <view class="allwrap" bindtap="goidentical"><text class="tilte">身份认证</text><text class="arrow"></text></view>
      
      <view class="allwrap" bindtap="gowechat"><text class="tilte">我的微信号</text><text class="arrow"></text></view>
      <view class="allwrap" bindtap=""><text class="tilte">帮助与反馈</text><text class="arrow"></text></view>
      
      <view class="allwrap" bindtap=""><text class="tilte">设置</text><text class="arrow"></text></view>
      
  </view>
